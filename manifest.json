{
  "manifest": "1",
  "version": "1.0.0",
  "locale": {
    "default": "en-us",
    "supported": [
      "en-us"
    ]
  },
  "elements": [
    {
      "path": "files",
      "name": "FAQ",
      "version": "1.0.0",
      "settings": {
        "config": {
          "external": {
            "url": "http://weebly.com",
            "label": "Check out Weebly."
          },
          "autopop": false
        },
        "properties": [
          {
            "type": "group",
            "name": "settings",
            "label": "Settings",
            "properties": [
              {
                "name": "items",
                "label": "Number of Items",
                "type": "int",
                "default": 3,
                "min": "1",
                "max": "100"
              },
              {
                "name": "style",
                "label": "Style",
                "type": "select",
                "default": "standard",
                "values": [
                  {
                    "name": "Simple",
                    "value": "simple"
                  },
                  {
                    "name": "Box",
                    "value": "box"
                  },
                  {
                    "name": "Material",
                    "value": "material"
                  }
                ]
              },
              {
                "name": "title_background",
                "label": "Title Background",
                "type": "color",
                "default": "#F9F9FA",
                "hidden": true
              },
              {
                "name": "content_background",
                "label": "Content Background",
                "type": "color",
                "default": "#FFFFF",
                "hidden": true
              }
            ]
          }
        ]
      }
    }
  ]
}